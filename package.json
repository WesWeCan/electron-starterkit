{
  "name": "electron-starterkit",
  "private": true,
  "description": "Electron Vue 3 + Vite Starterkit",
  "productName": "Electron Vue 3 + Vite Starterkit",
  "version": "1.0.0",
  "license": "MIT",
  "author": "WesWeCan",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite --config config/vite.ts",
    "dev:server": "vite --config config/vite-server.ts",
    "build": "yarn build:client && yarn build:app",
    "build:client": "vite build --config config/vite.ts",
    "build:app": "electron-builder -c config/electron-builder.json",
    "server": "node server/index.ts"
  },
  "dependencies": {
    "@types/puppeteer": "^7.0.4",
    "axios": "^1.4.0",
    "socket.io-client": "^4.7.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^6.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@vitejs/plugin-vue": "^4.2.3",
    "chrome-finder": "^1.0.7",
    "cors": "^2.8.5",
    "electron": "^27.1.3",
    "electron-builder": "^24.6.3",
    "express": "^4.18.2",
    "puppeteer": "^21.5.2",
    "sass": "^1.64.1",
    "socket.io": "^4.7.1",
    "tree-kill": "^1.2.2",
    "typescript": "^5.1.6",
    "vite": "4.5.0",
    "vite-plugin-electron": "^0.15.4",
    "ws": "^8.13.0"
  }
}
